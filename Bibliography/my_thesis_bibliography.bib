  author = {Hiroaki Ueda and Ryoya Suzuki and Atsushi Nakazawa and Yusuke Kurose and Murilo M. Marinho and Naoyuki Shono and Hirofumi Nakatomi and Nobuhito Saito and Eiju Watanabe and Akio Morita and Kanako Harada and Naohiko Sugita and Mamoru Mitsuishi},

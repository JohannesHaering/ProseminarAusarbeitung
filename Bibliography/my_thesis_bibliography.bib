@Article{force_feedback,
  author={A.M., Okamura},
  title={Methods for haptic feedback in teleoperated robot‐assisted surgery},
  journal={Industrial Robot: An International Journal},
  year={2004},
  month={Jan},
  day={01},
  publisher={Emerald Group Publishing Limited},
  volume={31},
  number={6},
  pages={499-508},
  abstract={Teleoperated minimally invasive surgical robots can significantly enhance a surgeon's accuracy, dexterity and visualization. However, current commercially available systems do not include significant haptic (force and tactile) feedback to the operator. This paper describes experiments to characterize this problem, as well as several methods to provide haptic feedback in order to improve surgeon's performance. There exist a variety of sensing and control methods that enable haptic feedback, although a number of practical considerations, e.g. cost, complexity and biocompatibility, present significant challenges. The ability of teleoperated robot‐assisted surgical systems to measure and display haptic information leads to a number of additional exciting clinical and scientific opportunities, such as active operator assistance through ``virtual fixtures'' and the automatic acquisition of tissue properties.},
  issn={0143-991X},
  doi={10.1108/01439910410566362},
  url={https://doi.org/10.1108/01439910410566362}
}

@ARTICLE{tactile_feedback,
  author={Z. F. {Quek} and W. R. {Provancher} and A. M. {Okamura}},
  journal={IEEE Transactions on Haptics}, 
  title={Evaluation of Skin Deformation Tactile Feedback for Teleoperated Surgical Tasks}, 
  year={2019},
  volume={12},
  number={2},
  pages={102-113},
  abstract={During interaction with objects using a tool, we experience force and tactile feedback. One form of tactile feedback is local fingerpad skin deformation. In this paper, we provide haptic feedback to users of a teleoperation system through a skin deformation tactile feedback device. The device is able to provide tangential and normal skin deformation in a coupled manner, and is designed so that users can grasp it with a precision grip using multiple fingerpads. By applying skin deformation feedback on multiple fingerpads, the device is able to provide multi-degree-of-freedom interaction force direction and magnitude information to the user. To evaluate the effectiveness of this approach for the performance of teleoperated manipulation tasks, we performed a study in which 20 participants used a teleoperation system to perform one of two manipulation tasks (peg transfer and tube connection) using force feedback, skin deformation feedback, and the combination of both feedback. Results showed that participants are able to use all feedback to improve task performance compared to the case without haptic feedback, although the degree of improvement depended on the nature of the task. The feedback also improved situation awareness, felt consistent with prior experience, and did not affect concentration on the task, as reported by participants.},
  keywords={dexterous manipulators;force feedback;haptic interfaces;medical robotics;surgery;tactile sensors;telerobotics;teleoperated surgical tasks;local fingerpad skin deformation;haptic feedback;skin deformation tactile feedback device;tangential skin deformation;normal skin deformation;multiple fingerpads;magnitude information;teleoperated manipulation tasks;force feedback;multidegree-of-freedom interaction force direction;Skin;Strain;Force;Manipulators;Task analysis;Force feedback;Tactile sensors;Haptics;skin deformation;tactile feedback;teleoperation},
  doi={10.1109/TOH.2018.2873398},
  ISSN={2329-4051},
  month={April},
}

